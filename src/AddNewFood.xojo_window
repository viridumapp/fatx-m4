#tag WindowCode
	#tag Event
		Sub Closing()
		  //Set buttons in Foodlist window when closing
		  Foodlist.Btn_delete.Enabled = False
		  Foodlist.Btn_new.Enabled = True
		  Foodlist.Foodbase_Selection.Enabled = True
		  
		End Sub
	#tag EndEvent

	#tag Event
		Sub Opening()
		  //Provide food group options based on foodlist selected (vegan/standard)
		  PopupMenuFoodGroup.RemoveAllRows
		  
		  If Foodlist.Foodbase_Selection.SelectedIndex = 0 Then
		    //Vegan 
		    PopupMenuFoodGroup.AddAllRows("Simple", "Complex", "Greens", "Protein", "Fats", "Legumes", "Extras")
		  ElseIf Foodlist.Foodbase_Selection.SelectedIndex =1 Then 
		    //Standard
		    PopupMenuFoodGroup.AddAllRows("Simple", "Complex", "Greens", "Protein", "Protein2", "Fats", "Extras")
		  End If
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Calckcal()
		  //Calculate the total kcal for the new food
		  
		  //Make it easy to understand
		  Var protein,carbs,fat As Double
		  
		  protein= TextFieldProtein.Text.ToDouble
		  carbs = TextFieldCarbs.Text.ToDouble
		  fat = TextFieldFat.Text.ToDouble
		  
		  Newfoodkcal = (protein*4)+(carbs*4)+(fat*9)
		  
		  //Check
		  Label6.Text = Newfoodkcal.ToString
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		Newfoodkcal As Double
	#tag EndProperty


#tag EndWindowCode

#tag Events TextFieldProtein
	#tag Event
		Sub FocusLost()
		  //Enter decimal as zero if missing
		  Me.Text = Format(Me.Text.ToDouble,"#.00")
		  
		  Calckcal()
		End Sub
	#tag EndEvent
#tag EndEvents
