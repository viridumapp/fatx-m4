		    Notestoclipboard = Totals_all.Text + Chr(13)
		    Notestoclipboard = Notestoclipboard + totals_all_percent.Text + Chr(13)+Chr(13)
		    
		    Notestoclipboard = Notestoclipboard + Label7.Text+ Chr(13)
