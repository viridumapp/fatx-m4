#tag Events Btn_commit
	#tag Event
		Sub Pressed()
		  //Check that fields for the new food are not missing
		  
		  If TextFieldFoodname.Text ="" Then
		    TextFieldFoodname.SetFocus
		    Return
		  End If
		  If PopupMenuFoodGroup.SelectedRowtext = "" Then
		    PopupMenuFoodGroup.SetFocus
		    Label6.Text ="Select group"
		    Return
		  End If
		  If TextFieldProtein.Text = "" Then
		    TextFieldProtein.setfocus
		    Return
		  End If
		  If TextFieldCarbs.Text = "" Then
		    TextFieldCarbs.setfocus
		    Return
		  End 
		  If TextFieldFat.Text = "" Then
		    TextFieldfat.SetFocus
		    Return
		  End If
		  
		  If Newfoodkcal = 0 Then
		    TextFieldProtein.SetFocus
		    Return
		  End If
		  
		  Calckcal()
		  
		  //Commit changes to foodlist
		  Var db As New SQLiteDatabase
		  db.Databasefile = New FolderItem(app.dbpath)
		  
		  Var dr As New DatabaseRow
		  
		  Try 
		    db.Connect
		    If dr <> Nil Then
		      //dr.IntegerColumn("id") = Foodlist.RecCount+1
		      dr.Column("name").Stringvalue = TextFieldFoodname.Text
		      dr.Column("kcal").DoubleValue = Newfoodkcal
		      dr.Column("protein").DoubleValue =  TextFieldProtein.Text.ToDouble
		      dr.Column("kulhydrat").DoubleValue = TextFieldCarbs.Text.ToDouble
		      dr.Column("fedt").DoubleValue = TextFieldFat.Text.ToDouble
		      dr.Column("gruppe").Stringvalue = PopupMenuFoodGroup.SelectedRowText
		      
		      db.AddRow(Foodlist.ListFood,dr)
		      
		      //Update the database for the mealplanner
		      Window1.Populate_selections
		    End If
		    
		    db.Commit
		    db.close
		    
		  Catch error As DatabaseException
		    Beep
		    MessageBox("Database Error: " + Str(db.ErrorCode) + EndOfLine + _
		    EndOfLine + db.ErrorMessage)
		  End Try
		  
		  //Set buttons in Foodlist window after commit save
		  Foodlist.Foodbase_Selection.Enabled = True
		  Foodlist.Btn_new.Enabled = True
		  Foodlist.Btn_delete.Enabled = False
		  
		  Foodlist.Populate_list
		  
		  //Close and leave current window
		  AddNewFood.close
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Btn_skip
	#tag Event
		Sub Pressed()
		  
		  //Skip adding new food and reset buttons in Foodlist window
		  Foodlist.Btn_new.Enabled = True
		  Foodlist.Btn_delete.Enabled = False
		  Foodlist.Foodbase_Selection.Enabled = True
		  
		  AddNewFood.close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PopupMenuFoodGroup
	#tag Event
		Sub SelectionChanged(item As DesktopMenuItem)
		  Calckcal()
		End Sub
	#tag EndEvent
#tag EndEvents
